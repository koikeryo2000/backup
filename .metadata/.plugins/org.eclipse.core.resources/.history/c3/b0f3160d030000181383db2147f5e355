package action;

import java.util.Map;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;

public class NetMoneyCreateConfirmAction extends ActionSupport implements SessionAware {
	public Map<String, Object> sessiona;

	private int price;

	public String execute(){
		String result = SUCCESS;

		sessiona.put("price", price);



		return result;
	}



	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}



	public Map<String, Object> getSessiona() {
		return sessiona;
	}



	public void setSessiona(Map<String, Object> sessiona) {
		this.sessiona = sessiona;
	}



	@Override
	public void setSession(Map<String, Object> arg0) {
		// TODO 自動生成されたメソッド・スタブ

	}



}
