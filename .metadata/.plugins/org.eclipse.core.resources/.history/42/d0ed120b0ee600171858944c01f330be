package nigen;

public class koike2 {

	public static void main(String[] args) {
		Peroson koike = new Peroson();

		koike.setName("小池");
		koike.setSeibetu("男");
		koike.setSyumi("game");
		koike.setTodouhuken("東京");

		System.out.println(koike.getName()+koike.getSeibetu()+koike.getSyumi()+koike.getTodouhuken());


	}

}
